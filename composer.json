{
    "name": "nmrkt/linkshare",
    "description": "A guzzle client for consuming the Commission Junction API",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/michalmarcinkowski/guzzle5-oauth2-subscriber"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "~5.0",
        "guzzlehttp/guzzle-services": "0.4.*",
        "michalmarcinkowski/guzzle5-oauth2-subscriber": "dev-master"

    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ian Fosbery",
            "email": "ian@nmrkt.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Nmrkt\\Linkshare": "src/",
            "Nmrkt\\Tests": "tests/"
        }
    }
}

{
    "name": "nmrkt/linkshare",
    "description": "A guzzle client for consuming the Commission Junction API",
    "require": {
        "guzzlehttp/guzzle": "~5.0",
        "guzzlehttp/guzzle-services": "0.4.*",
        "fosberyi/guzzle5-oauth2-subscriber": "master"

    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "nmrkt/guzzle5-oauth2-subscriber",
                "version": "master",
                "source": {
                    "url": "https://github.com/fosberyi/guzzle5-oauth2-subscriber",
                    "type": "git",
                    "reference": "master"
                }
            }

        }
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ian Fosbery",
            "email": "ian@nmrkt.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Nmrkt\\Linkshare": "src/",
            "Nmrkt\\Tests": "tests/",
            "Nmrkt\\GuzzleOAuth2": "vendor/fosberyi/guzzle5-oauth2-subscriber/src/"
        }
    }
}
